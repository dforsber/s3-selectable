{
  "name": "@dforsber/s3-selectable",
  "version": "0.1.5",
  "description": "S3 Select over Glue Table",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts",
  "module": "dist/esm/index.js",
  "author": "dforsber",
  "license": "MIT",
  "private": false,
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dforsber/s3-selectable"
  },
  "keywords": [
    "S3",
    "Glue Table"
  ],
  "scripts": {
    "prebuild": "yarn install --frozen-lockfile",
    "build": "rimraf dist/ && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "build:watch": "npx onchange 'src/**/*.ts' -- yarn build",
    "lint": "eslint \"./src/**/*.ts\" --fix",
    "lint:watch": "npx onchange 'src/**/*.ts' -- yarn lint",
    "test": "jest",
    "test:watch": "yarn test -- --watchAll",
    "test:badges": "npx jest-coverage-badges output ./badges",
    "clean": "rimraf dist/ node_modules/ coverage/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "dependencies": {
    "aws-sdk": "^2.673.0",
    "merge-stream": "^2.0.0",
    "node-sql-parser": "^1.11.0",
    "sqlite3": "^4.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@types/jest": "^25.2.1",
    "@types/merge-stream": "^1.1.2",
    "@types/node": "^13.13.5",
    "@types/node-sql-parser": "^1.1.0",
    "@types/sqlite3": "^3.1.6",
    "aws-sdk-mock": "^5.1.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.10.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.0.1",
    "onchange": "^7.0.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.5.1",
    "typescript": "^3.9.0"
  }
}
